{
  "name": "weighthack",
  "version": "1.0.0",
  "description": "WeightHack Project",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=developer concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "react",
    "mern"
  ],
  "author": "Команда «Умелые ручки» Московской предпрофессиональной олимпиады",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.16",
    "react-moment": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
